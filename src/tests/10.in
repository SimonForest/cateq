x := gen *
y := gen *
z := gen *
a := gen x -> y
b := gen x -> y
c := gen x -> y
d := gen y -> z
e := gen y -> z
f := gen y -> z
al := gen a -> b
alp := gen a -> b
be := gen b -> c
bep := gen b -> c
ga := gen d -> e
gap := gen d -> e
de := gen e -> f
dep := gen e -> f
A := gen al *0 de -> alp *0 dep
B := gen be *0 ga -> bep *0 gap
Aext1 := ( a *0 ga ) *1 A *1 ( be *0 f )
Aext2 := ( a *0 gap ) *1 A *1 ( bep *0 f )
Bext1 := ( al *0 d ) *1 B *1 ( c *0 de )
Bext2 := ( alp *0 d ) *1 B *1 ( c *0 dep )
% Aext1 *2 Bext2 = Bext1 *2 Aext2
% Aext1 = { a, ga, be, f, A }
% Aext1 = { a, gap, be, f, A }
{ A , be, ga } *2 { B, alp, dep } = { B, al, de } *2 { A, bep, gap } % false
{ A , be, ga } *2 { B, alp, dep } = { A , be, ga } *2 { B, alp, dep } % true